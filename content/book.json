{
    "plugins": [
        "github",
        "theme-comscore",
        "chapter-fold",
        "code",
        "-search",
        "-lunr",
        "search-pro",
        "-sharing",
        "sharing-plus",
        "tbfed-pagefooter",
        "simple-page-toc",
        "ancre-navigation",
        "pageview-count",
        "favicon"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/dalongm"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "pocket": false,
            "qq": false,
            "qzone": true,
            "twitter": false,
            "weibo": true,
            "all": [
                "weibo",
                "qzone",
                "douban",
                "facebook",
                "google",
                "instapaper",
                "linkedin",
                "messenger",
                "qq",
                "viber",
                "whatsapp"
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy dalongm.top 2020",
            "modify_label": "修改时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "favicon": {
            "shortcut": "images/favicon.ico",
            "bookmark": "images/favicon.ico"
        }
    }
}